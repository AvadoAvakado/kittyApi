<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="applicationinterface.controllers.RandomKittyFilterPopupController"
      prefHeight="300.0" prefWidth="200.0">
    <HBox fx:id="columnContainer" VBox.vgrow="ALWAYS">
        <VBox fx:id="leftColumn" HBox.hgrow="ALWAYS"/>
        <VBox fx:id="rightColumn" HBox.hgrow="ALWAYS"/>
    </HBox>
    <HBox VBox.vgrow="NEVER">
        <Button fx:id="applyButton" text="Apply" onAction="#applyButtonClick" HBox.hgrow="ALWAYS"
                VBox.vgrow="ALWAYS" maxWidth="Infinity"/>
        <Button fx:id="cancelButton" text="Cancel" onAction="#cancelButtonClick" HBox.hgrow="ALWAYS"
                VBox.vgrow="ALWAYS" maxWidth="Infinity"/>
    </HBox>
</VBox>

